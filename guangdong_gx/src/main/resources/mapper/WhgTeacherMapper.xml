<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.creatoo.hn.dao.mapper.WhgTraTeacherMapper">

    <select id="t_srchTeaList" resultType="hashmap">
        SELECT
            t1.*
        FROM
            whg_tra_teacher t1
        WHERE
            t1.id NOT IN (
                SELECT
                    c1.entid
                FROM
                    whg_major_contact c1
                WHERE
                    c1.type = 2
                    AND c1.majorid = #{mid}
            )
        AND t1.state = 6
         AND t1.cultid = #{cultid}
    </select>
</mapper>